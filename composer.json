{
    "name":        "arcanedev/laravel-metrics",
    "description": "Provides a Metrics for Laravel 5",
    "keywords":    ["arcanedev", "arcanesoft", "laravel", "metrics", "data", "value", "trends"],
    "homepage":    "https://github.com/ARCANEDEV/LaravelMetrics",
    "authors": [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "arcanedev/support": "^8.0",
        "cakephp/chronos": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\LaravelMetrics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\LaravelMetrics\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "coverage": "phpunit --coverage-html build/coverage/html"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "3.x-dev"
        },
        "laravel": {
            "providers": [
                "Arcanedev\\LaravelMetrics\\MetricServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
